<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<Configuration packages="id.dkakunsi.lab.jlogging" name="jlogging">
    <Appenders>
        <Console name="CONSOLE" target="SYSTEM_OUT">
            <StandardizedLayout />
        </Console>
    </Appenders>
    <Loggers>
        <Root level="INFO">
            <AppenderRef ref="CONSOLE" level="INFO" />
        </Root>
    </Loggers>
</Configuration>
